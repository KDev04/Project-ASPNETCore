@model List<Laptop>
@{
    ViewData["Title"] = "Home Page";
}

<div class="container p-3 ">
    <div class="row ">
        <ul class="nav " style="padding-left: 10px;">
            <li class="nav-item ">
                <a style="margin-right: 4px; color: black; font-family: 'Noto Sans', sans-serif;" class=" nav-link px-0"
                    aria-current="page" href="/">Home /</a>
            </li>
            <li class="nav-item ">
                <a style="margin-right: 4px; color: black; font-family: 'Noto Sans', sans-serif; " class="nav-link px-0"
                    href="/Laptop"> Store /</a>
            </li>
            <li class="nav-item ">
                <a style="margin-right: 4px; color: black; font-family: 'Noto Sans', sans-serif;" class="nav-link px-0"
                    href="/Laptop">Predator Desktop </a>
            </li>
        </ul>
    </div>

</div>


<section class="container ">
    <partial name="~/Views/Laptop/Filter.cshtml" />
</section>

<section class="container">

    <div class="row row-cols-4 py-3">
        @foreach (var laptop in Model)
        {
            <partial name="LaptopSummary.cshtml" model="laptop" />

        }
    </div>
</section>
